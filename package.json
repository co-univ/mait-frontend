{
	"name": "mait-frontend",
	"version": "1.0.0",
	"private": true,
	"type": "module",
	"scripts": {
		"build": "pnpm run build:tokens && rsbuild build",
		"build:tokens": "node tools/simple-tokens-parser.cjs",
		"dev": "pnpm run build:tokens && rsbuild dev --open",
		"preview": "rsbuild preview",
		"lint": "biome lint --write",
		"format": "pnpm format:biome && pnpm format:tailwind",
		"format:biome": "biome format --write",
		"format:tailwind": "prettier --plugin=prettier-plugin-tailwindcss --write \"src/**/*.{js,ts,jsx,tsx}\"",
		"check": "biome check --write",
		"reporter": "biome check --reporter=summary",
		"generate:types": "node tools/generate-types.js"
	},
	"dependencies": {
		"@hello-pangea/dnd": "^18.0.1",
		"@tailwindcss/typography": "^0.5.16",
		"@tanstack/react-query": "^5.84.0",
		"clsx": "^2.1.1",
		"lodash.merge": "^4.6.2",
		"clsx": "^2.1.1",
		"framer-motion": "^12.23.12",
		"lucide-react": "^0.525.0",
		"react": "^19.1.0",
		"react-dom": "^19.1.0",
		"react-router-dom": "^7.7.0",
		"react-toastify": "^11.0.5"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.0.5",
		"@openapitools/openapi-generator-cli": "^2.21.4",
		"@rsbuild/core": "^1.3.22",
		"@rsbuild/plugin-react": "^1.3.2",
		"@types/node": "^24.0.7",
		"@types/react": "^19.1.8",
		"@types/react-dom": "^19.1.6",
		"autoprefixer": "^10.4.21",
		"dotenv": "^17.2.1",
		"openapi-typescript": "^7.8.0",
		"postcss": "^8.5.6",
		"postcss-loader": "^8.1.1",
		"prettier": "^3.6.2",
		"prettier-plugin-tailwindcss": "^0.6.14",
		"style-dictionary": "^5.0.1",
		"tailwindcss": "^3.4.17",
		"typescript": "^5.8.3"
	}
}
